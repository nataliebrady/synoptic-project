<% provide(:title, 'All quizzes') %>

<div class="row mainpage-container">

  <% flash.each do |message_type, message| %>
      <%= content_tag(:div, message, class: "alert form-alert alert-#{message_type}") %>
  <% end %>
  
<div class="col-md-12">

<h1>All quizzes</h1>

<br>

<%= will_paginate %>

<ul class="users">
  <% @quizzes.each do |quiz| %>
      <%= render quiz %>
  <% end %>
</ul>

<%= will_paginate %>

</div>
</div>
